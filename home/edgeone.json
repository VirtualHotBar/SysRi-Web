{
    "redirects": [
      {
        "source": "$wwwhost",
        "destination": "$host",
        "statusCode": 301
      },
      {
        "source": "/Custom/",
        "destination": "/custom/",
        "statusCode": 301
      },
      {
        "source": "/QQGroup/",
        "destination": "/com-group/",
        "statusCode": 301
      }
    ],
  "caches": [
    {
      "source": "/*",
      "cacheTtl": 604800
    }
  ],
  "headers": [
    {
      "source": "/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=2592000"
          }
        ]
    }
  ]
}
